{
    "name": "survey-engine",
    "version": "1.0.0",
    "private": true,
    "description": "A survey platform with React frontend and NestJS backend",
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "build": "npm run build --workspaces",
        "build:web": "npm run build --workspace=apps/web",
        "build:api": "npm run build --workspace=apps/api",
        "start:api": "npm run start --workspace=apps/api",
        "start:dev": "npm run start:dev --workspaces",
        "start:web:dev": "npm run start:dev --workspace=apps/web",
        "start:api:dev": "npm run start:dev --workspace=apps/api"
    },
    "keywords": [
        "survey",
        "forms",
        "typescript",
        "monorepo"
    ],
    "author": "",
    "license": "ISC"
}
